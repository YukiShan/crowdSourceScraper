	$$('.njob').each( function(item) {
		var uuid = item.readAttribute('data-uuid');
		var hcolor = item.readAttribute('data-hcolor');
		var featured = item.readAttribute('data-featured');
		var title = item.innerHTML;
		
		var html = '<a href="/JobDetails/'+uuid+'" onclick="return GB_showPage(\'Job Details\', this.href); return false;">';
		if(hcolor.length > 0){
			html += '<span class="sp_item">';
