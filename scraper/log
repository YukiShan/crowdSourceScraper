<script type="text/javascript">
function prettyDate(date_time){
	var date = new Date(processDate(date_time));
	var diff = (((new Date()).getTime() - date.getTime()) / 1000),
	day_diff = Math.floor(diff / 86400),
	out = "Some time ago";

	switch(true) {
			case diff < 60: 	 out = "Just now"; break;
			case diff < 120: 	 out = "1 minute ago"; break;
			case diff < 3600: 	 out = Math.floor( diff / 60 ) + " minutes ago"; break;
			case diff < 7200: 	 out = "1 hour ago"; break;
			case diff < 86400: 	 out = Math.floor( diff / 3600 ) + " hours ago"; break;
			case day_diff == 1:  out = "Yesterday"; break;
			case day_diff < 7: 	 out = day_diff + " days ago"; break;
			case day_diff < 31:  out = Math.ceil( day_diff / 7 ) + " weeks ago"; break;
			case day_diff < 366: out = Math.floor( day_diff / 30 ) + " months ago"; break;
			case day_diff > 365: out = Math.floor( day_diff / 365 ) + " years ago"; break;
	};

	return out;
	function processDate(cdate){
		var date_time_arr = cdate.split(' ');
		var date_arr = date_time_arr[0].split('-');
		var time_arr = date_time_arr[1].split(':');
		
		var final_date = parseInt(date_arr[1], 10)+'/'+parseInt(date_arr[2],10)+'/'+date_arr[0];

		if(parseInt(time_arr[0], 10) < 12){
			var final_time =  parseInt(time_arr[0], 10)+':'+parseInt(time_arr[1],10)+':'+time_arr[2]+' AM UTC';
		} else {
			var final_time =  parseInt((time_arr[0]-12), 10)+':'+parseInt(time_arr[1],10)+':'+time_arr[2]+' PM UTC';
		}
		
		return final_date + ' ' + final_time
	}
}
function strip_tags (input, allowed) {
  allowed = (((allowed || "") + "").toLowerCase().match(/<[a-z][a-z0-9]*>/g) || []).join(''); // making sure the allowed arg is a string containing only tags in lowercase (<a><b><c>)
  var tags = /<\/?([a-z][a-z0-9]*)\b[^>]*>/gi,
    commentsAndPhpTags = /<!--[\s\S]*?-->|<\?(?:php)?[\s\S]*?\?>/gi;
  return input.replace(commentsAndPhpTags, '').replace(tags, function ($0, $1) {
    return allowed.indexOf('<' + $1.toLowerCase() + '>') > -1 ? $0 : '';
  });
}
</script>
