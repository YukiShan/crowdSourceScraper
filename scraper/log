			cpayment.push(item.innerHTML);
		}
	});
	cpayment.sort(function(a,b){ a=a.replace('$','');b=b.replace('$',''); return parseFloat(b)-parseFloat(a);});

	drawSelectField('head_payment', '4', cpayment);
	
	
	/* *********************** */
	var ctimereq = [];
	$$('.row .i_5').each( function(item) {
		if(ctimereq.indexOf(item.innerHTML)==-1){
			ctimereq.push(item.innerHTML);
		}
	});
	ctimereq.sort(function(a,b){ a=a.replace('min','');b=b.replace('min',''); return parseFloat(b)-parseFloat(a);});
	
	drawSelectField('head_time', '5', ctimereq);
	
});

function drawSelectField(cid, col_id ,coptions){
	var s_select = '<select id="select_'+col_id+'" onchange="filterTableRow()"><option style="color: #D00; text-decoration:underline;" class="hidden-option" value="">'+$(cid).innerHTML+'</option>';
	while (opt = coptions.pop()){
		s_select +='<option value="'+opt+'">'+opt+'</option>';
	}
	s_select += '</select>';
	$(cid).update(s_select);
}

function job_ksearch(e){
	if (e.keyCode == 13) {
		job_search();
	}
}

function job_search(){
	filterTableRow();
}
function filterTableRow(){
	var start_i = 0;
	for(var i=2; i<= 5;i++){
		if($('select_'+i).getValue().length > 0){
			if(start_i == 0){
				start_i = i;
			}
			$$('.row .i_'+i).each( function(item) {
				if(item.innerHTML == $('select_'+i).getValue() && (item.up().visible() || i==start_i) ){
					item.up().show();
				} else {
					item.up().hide();
				}
			});
		}
	}
	
	search_input = $('jnsearch').getValue().toLowerCase();
	if(search_input.length != 0){
		$$('.njob').each( function(item) {
			if(item.innerHTML.toLowerCase().indexOf(search_input)>-1 && item.up().visible()){
				item.up().show();
			} else {
				item.up().hide();
			}
		});
	} else if(start_i == 0){
		$$('.row').each( function(item) {
			item.show();
		});	
	}
}
